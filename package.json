{
  "name": "api-express",
  "version": "0.0.0",
  "description": "API con nodejs por Alejandro",
  "keywords": [
    "API con nodejs",
    "API"
  ],
  "main": "src/app.js",
  "author": "Alejandro <g4alejandro29@gmail.com>",
  "private": true,
  "files": [
    "src"
  ],
  "scripts": {
    "local:server": "cross-env DEBUG=* nodemon --watch src --inspect src/app.js",
    "local:db:migrate": "sequelize db:migrate --env local --config src/config/config.json",
    "local:db:migrate:undo": "sequelize db:migrate:undo --env local --config src/config/config.json",
    "local:db:migrate:undo:all": "sequelize db:migrate:undo:grate --env local --config src/config/config.json",
    "local:seed": "sequelize db:seed:all --env local --config src/config/config.json",
    "local:seed:undo": "sequelize db:seed:undo --env  local --config src/config/config.json",
    "local:seed:undo:all": "sequelize db:seed:undo:all --env local --config src/config/config.json",
    "local:moving:account": "node src/jobs/joinAccount.js",
    "start": "ntl"
  },
  "dependencies": {
